pipeline {
    agent any
    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                // Tambahkan perintah build di sini, misalnya `mvn clean install`
            }
        }
        stage('Test') {
            steps {
                echo 'Testing...'
                // Tambahkan perintah testing di sini, misalnya `mvn test`
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying...'
                // Tambahkan perintah deploy di sini
            }
        }
    }
}
